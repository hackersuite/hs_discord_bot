{
  "name": "hackathon-discord-bot",
  "version": "1.0.0",
  "description": "A Discord bot to assist with the online StudentHack 2020",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run test",
    "build": "rimraf dist && tsc",
    "test": "eslint src --ext .ts",
    "start": "npm run build && node . | pino-pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amishshah/hackathon-bot.git"
  },
  "keywords": [
    "Discord",
    "Hackathon",
    "StudentHack"
  ],
  "author": "Amish Shah <amishshah.2k@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amishshah/hackathon-bot/issues"
  },
  "homepage": "https://github.com/amishshah/hackathon-bot#readme",
  "devDependencies": {
    "@types/node": "^13.9.5",
    "@types/pino": "^5.17.0",
    "@types/ws": "^7.2.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-marine": "^6.0.0",
    "pino-pretty": "^3.6.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "extends": "marine/node",
    "parserOptions": {
      "project": "tsconfig.eslint.json"
    },
    "env": {
      "jest": true
    },
    "rules": {
      "@typescript-eslint/no-require-imports": "off",
      "@typescript-eslint/no-var-requires": "off"
    }
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.5",
    "@unicsmcr/hs_discord_bot_api_client": "^0.0.2",
    "bad-words": "^3.0.3",
    "canvas": "^2.6.1",
    "discord-akairo": "^8.0.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "form-data": "^3.0.0",
    "node-fetch": "^2.6.0",
    "pino": "^5.17.0"
  }
}
