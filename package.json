{
  "name": "hs_discord_bot",
  "version": "1.0.0",
  "description": "A Discord bot to assist with the online StudentHack 2020",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run test",
    "build": "rimraf dist && tsc",
    "test": "npm run lint",
    "lint": "eslint src --ext .ts",
    "start": "npm run build && node . | pino-pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unicsmcr/hs_discord_bot.git"
  },
  "keywords": [
    "Discord",
    "Hackathon",
    "StudentHack"
  ],
  "author": "Amish Shah <amishshah.2k@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/unicsmcr/hs_discord_bot/issues"
  },
  "homepage": "https://github.com/unicsmcr/hs_discord_bot#readme",
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@types/pino": "^6.3.0",
    "@types/ws": "^7.2.5",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@unicsmcr/eslint-config": "^0.0.1",
    "eslint": "^7.3.1",
    "pino-pretty": "^4.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  },
  "eslintConfig": {
    "extends": "@unicsmcr",
    "parserOptions": {
      "project": "tsconfig.eslint.json"
    },
    "env": {
      "jest": true
    },
    "rules": {
      "@typescript-eslint/no-require-imports": "off",
      "@typescript-eslint/no-var-requires": "off"
    }
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "@unicsmcr/hs_discord_bot_api_client": "^0.0.4",
    "bad-words": "^3.0.3",
    "canvas": "^2.6.1",
    "discord-akairo": "^8.0.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "form-data": "^3.0.0",
    "humanize-duration": "^3.23.1",
    "node-fetch": "^2.6.0",
    "pino": "^6.3.2"
  }
}
